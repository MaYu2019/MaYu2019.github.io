$(document).ready(function(){
	function ShuJu(localIp) {	
	var localUrl = "http://messageboard.peopletech.cn/answered/new_answered_all.jsonp?callback=IndexThreads&_=1584661773758"
	$.ajax({
		type: "get",
		url: localUrl,
		timeout : 3000,
		cache: false,
		dataType: "jsonp",
		jsonp: "callback",
		jsonpCallback: "IndexThreads",
		success: function(data) {
			var hftime;
			var arr = [];
			var size = 10;
			var sum = 5;			
			while(true){
				var num = Math.floor(Math.random()*size);
				if (!Array.indexOf) {
					Array.prototype.indexOf = function (obj) {
						for(var i = 0;i < this.length;i++){
							if (this[i] == obj) {
								return i;
							}
						}
						return -1;
					}
				}				
				if(arr.indexOf(num) == -1){
					arr.push(num)
				}				
				if(arr.length == sum){
					break;
				}
			}
			arr.sort(function(a, b){return a - b});		
			var localHtml='<a href="http://leaders.people.com.cn/GB/178291/218130/414212/index.html" target="_blank"><img src="/img/2019peopleindex/images/logo/lyb.png" width="140" height="30" style="margin:0px 30px 10px 0;vertical-align:top;float:left;" /></a><div class="winBox"><ul class="scroll" id="a01">';
			var zlength=0;
			$.each(data,function(i,n){
				if (i == 5) {
					 return false;
				}
				var myDate = new Date();
				var mytime= parseInt(myDate.getTime()/1000)-data[arr[i]].answerTime;
				var todaytime=myDate.getHours()*3600+myDate.getMinutes()*60+myDate.getSeconds();
				var zttime=todaytime+86400;				
				var sgwidth;
				if(mytime < 3600){
					hftime="刚刚  ";
				}
				else if(mytime > 3600  && mytime < todaytime){
					hftime="今天  ";
				}
				else if(mytime > todaytime && mytime < zttime){
					hftime="昨天  ";
				}
				else if(mytime > zttime){
					hftime="";
				}
				if(data[arr[i]].typeName == "感谢"){
					sgwidth=parseInt(data[arr[i]].forumName.length*14+220);
					localHtml += '<li style="width:'+ sgwidth+'px">'+ hftime +'<a href="'+ data[arr[i]].threadUrl +'" target="_blank">网友给'+ data[arr[i]].forumName +'发来感谢信</a></li>';					
				}else{
				sgwidth=parseInt(data[arr[i]].forumName.length*14+300);
				localHtml += '<li style="width:'+ sgwidth+'px">'+ hftime +'<a href="'+ data[arr[i]].threadUrl +'" target="_blank">网友给'+ data[arr[i]].forumName +'反映的“'+ data[arr[i]].domainName +'”问题获回应</a></li>';
				}
				zlength=sgwidth+zlength;
			});
			localHtml +='</ul></div>'
			$("#ldlyb").html(localHtml);
			var num = 0;
			zlength=780-zlength;
			console.log(zlength);
			function goLeft() {
				if (num == zlength) {
					num = 0;
				}
				num -= 1;
				$(".scroll").css({
					left: num
				})
			}
			var timer = setInterval(goLeft,20);
			//设置鼠标经过时滚动停止
			$(".scroll").hover(function() {
				clearInterval(timer);
			},
			function() {
				timer = setInterval(goLeft,20);
			})
		},
		error: function(XMLHttpRequest, textStatus, errorThrown) {
			console.log(XMLHttpRequest.status);
			console.log(XMLHttpRequest.readyState);
			console.log(textStatus);
		}
	});
  }
ShuJu();	
})
